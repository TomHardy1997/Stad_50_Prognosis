   "cell_type": "code",
   "execution_count": 9,
   "id": "8ac425b8-674f-42a5-b8ff-904815850670",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "月数差值: 67.39\n"
     ]
    }
   ],
   "source": [
    "from dateutil import relativedelta\n",
    "from datetime import datetime\n",
    "\n",
    "# 定义两个日期\n",
    "date1 = datetime.strptime(\"2021-09-01\", \"%Y-%m-%d\")\n",
    "date2 = datetime.strptime(\"2016-01-20\", \"%Y-%m-%d\")\n",
    "\n",
    "# 计算两个日期之间的差值\n",
    "diff = relativedelta.relativedelta(date1, date2)\n",
    "\n",
    "# 计算月数差值\n",
    "months = diff.years * 12 + diff.months + diff.days / 30.44\n",
    "\n",
    "# 输出月数差值，精确到小数点后两位\n",
    "print(f\"月数差值: {months:.2f}\")\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "id": "f8fa7ceb-040c-48de-b5fa-86f81d0c7cb1",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "from dateutil import relativedelta\n",
    "from datetime import datetime\n",
    "\n",
    "# 假设你的DataFrame是new_df，两列日期是date1和date2\n",
    "new_df['surgery_time'] = pd.to_datetime(new_df['surgery_time'])\n",
    "new_df['last_follow'] = pd.to_datetime(new_df['last_follow'])\n",
    "\n",
    "# 计算两列日期的差值，结果是一个timedelta对象\n",
    "new_df['diff'] = new_df['last_follow']- new_df['surgery_time']\n",
    "\n",
    "# 定义一个函数，将timedelta对象转换为月数\n",
    "def timedelta_to_months(td):\n",
    "    return td.days / 30.44\n",
    "\n",
    "# 使用apply函数，将timedelta对象转换为月数\n",
    "new_df['months'] = new_df['diff'].apply(timedelta_to_months)\n",
    "\n",
    "# 输出结果，精确到小数点后两位\n",
    "new_df['months'] = new_df['months'].round(2)\n"
   ]
  },
